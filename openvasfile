pipeline {
  agent any 
  tools {
    maven 'maven'
  }

  stages {
    stage ('Initialize') {
        steps {
            sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
            ''' 
      }
    }
    
    stage('openvas'){
        steps{
                sh 'sudo docker exec -it openvas omp --xml \\'<start_task task_id="6c698f59-810f-40d7-b1bc-5986170dd246"/>\\''
            }
         }    

   
    }
}
